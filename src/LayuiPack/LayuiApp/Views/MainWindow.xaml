<Lay:LayWindow
    x:Class="LayuiApp.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Global="clr-namespace:LayUI.Wpf.Global;assembly=LayUI.Wpf"
    xmlns:Lay="clr-namespace:LayUI.Wpf.Controls;assembly=LayUI.Wpf"
    xmlns:Nv="clr-namespace:Layui.Core.Resource;assembly=Layui.Core"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:local="clr-namespace:LayuiApp.Views"
    xmlns:prism="http://prismlibrary.com/"
    Width="1080"
    Height="600"
    prism:ViewModelLocator.AutoWireViewModel="True"
    HeaderBackground="{DynamicResource LighCyan}"
    IsShowHeader="True"
    ShowInTaskbar="True"
    WindowStartupLocation="CenterOwner"
    WindowState="{Binding WindowState}">
    <Lay:LayWindow.HeaderEffect>
        <DropShadowEffect
            BlurRadius="3"
            Direction="270"
            ShadowDepth="0" />
    </Lay:LayWindow.HeaderEffect>
    <Lay:LayWindow.HeaderContent>
        <Grid WindowChrome.IsHitTestVisibleInChrome="False">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <TextBlock
                Margin="10,0"
                VerticalAlignment="Center"
                FontFamily="{DynamicResource IconFont}"
                FontSize="20"
                Foreground="White"
                Text="&#xeeba;" />
            <TextBlock
                Grid.Column="1"
                VerticalAlignment="Center"
                FontSize="14"
                Foreground="White"
                Text="{Binding Title}" />
            <StackPanel
                Grid.Column="2"
                HorizontalAlignment="Right"
                Orientation="Horizontal"
                WindowChrome.IsHitTestVisibleInChrome="true">
                <Lay:LayBadge
                    Margin="5,10"
                    Background="{DynamicResource Red}"
                    IsDot="True"
                    Value="1">
                    <ToggleButton
                        x:Name="Title"
                        Content="公告"
                        Cursor="Hand"
                        Foreground="White">
                        <ToggleButton.Template>
                            <ControlTemplate TargetType="ToggleButton">
                                <Grid Background="Transparent">
                                    <ContentPresenter VerticalAlignment="Center" />
                                </Grid>
                            </ControlTemplate>
                        </ToggleButton.Template>
                    </ToggleButton>
                </Lay:LayBadge>
                <Grid VerticalAlignment="Center">
                    <ToggleButton
                        x:Name="MoreToggleButton"
                        Width="50"
                        Content="&#xed3b;"
                        Cursor="Hand"
                        FontFamily="{StaticResource IconFont}"
                        FontSize="20"
                        Foreground="White">
                        <ToggleButton.ToolTip>
                            <TextBlock FontSize="14" Text="更多" />
                        </ToggleButton.ToolTip>
                        <ToggleButton.Template>
                            <ControlTemplate TargetType="ToggleButton">
                                <Grid Background="Transparent">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Grid>
                            </ControlTemplate>
                        </ToggleButton.Template>
                        <ToggleButton.Style>
                            <Style TargetType="ToggleButton">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=MorePopup, Path=IsOpen}" Value="true">
                                        <Setter Property="IsEnabled" Value="false" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </ToggleButton.Style>
                    </ToggleButton>
                    <Popup
                        x:Name="MorePopup"
                        AllowsTransparency="True"
                        IsOpen="{Binding ElementName=MoreToggleButton, Path=IsChecked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Placement="Bottom"
                        PlacementTarget="{Binding ElementName=MoreToggleButton}"
                        PopupAnimation="Slide"
                        StaysOpen="False"
                        VerticalOffset="5">
                        <Popup.Style>
                            <Style TargetType="Popup">
                                <Setter Property="IsOpen" Value="false" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=MoreToggleButton, Path=IsChecked}" Value="true">
                                        <Setter Property="IsOpen" Value="true" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Popup.Style>
                        <Grid>
                            <Grid.Effect>
                                <DropShadowEffect Opacity="0.5" ShadowDepth="0" />
                            </Grid.Effect>
                            <Grid
                                Width="10"
                                Height="10"
                                Margin="20,2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Background="White"
                                RenderTransformOrigin="0.5,0.5">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform />
                                        <SkewTransform />
                                        <RotateTransform Angle="45" />
                                        <TranslateTransform />
                                    </TransformGroup>
                                </Grid.RenderTransform>
                            </Grid>
                            <Border
                                Width="150"
                                Height="auto"
                                Margin="5"
                                Background="White"
                                CornerRadius="{DynamicResource ControlCornerRadiusBase}">
                                <StackPanel>
                                    <StackPanel.Resources>
                                        <Style TargetType="Button">
                                            <Setter Property="Width" Value="50" />
                                            <Setter Property="Height" Value="50" />
                                            <Setter Property="Background" Value="Transparent" />
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="Button">
                                                        <Border Background="{TemplateBinding Background}">
                                                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                                                        </Border>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                        <Style x:Key="WindowCheckBoxStyle" TargetType="CheckBox">
                                            <Setter Property="Width" Value="50" />
                                            <Setter Property="Height" Value="50" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="CheckBox">
                                                        <Border Background="Transparent">
                                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                        </Border>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </StackPanel.Resources>
                                    <Button
                                        Width="150"
                                        Height="40"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding GoBrowser}"
                                        CommandParameter="https://github.com/Layui-WPF-Team/Layui-WPF">
                                        <Grid Margin="30,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontFamily="{DynamicResource IconFont-FontAwesome}"
                                                FontSize="18"
                                                Text="&#xf09b;" />
                                            <TextBlock
                                                Grid.Column="1"
                                                Margin="10,0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="GitHub" />
                                        </Grid>
                                    </Button>
                                    <Button
                                        Width="150"
                                        Height="40"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding GoBrowser}"
                                        CommandParameter="https://www.cnblogs.com/ShyFrog/">
                                        <Grid Margin="30,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontFamily="{DynamicResource IconFont-FontAwesome}"
                                                FontSize="18"
                                                Text="&#xf127;" />
                                            <TextBlock
                                                Grid.Column="1"
                                                Margin="10,0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="博客" />
                                        </Grid>
                                    </Button>
                                    <Button
                                        Width="150"
                                        Height="40"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding GoBrowser}"
                                        CommandParameter="https://b23.tv/F9IA5Vs">
                                        <Grid Margin="30,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontFamily="{DynamicResource IconFont-FontAwesome}"
                                                FontSize="18"
                                                Text="&#xf03d;" />
                                            <TextBlock
                                                Grid.Column="1"
                                                Margin="10,0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="教学视频" />
                                        </Grid>
                                    </Button>
                                    <Button
                                        Width="150"
                                        Height="40"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding GoQQ}">
                                        <Grid Margin="30,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                FontFamily="{DynamicResource IconFont-FontAwesome}"
                                                FontSize="18"
                                                Text="&#xf1d6;" />
                                            <TextBlock
                                                Grid.Column="1"
                                                Margin="10,0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="QQ群" />
                                        </Grid>
                                    </Button>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </Popup>
                </Grid>
            </StackPanel>
        </Grid>
    </Lay:LayWindow.HeaderContent>
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding LoadedCommand}" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Lay:LayNoticeBar
            Height="40"
            Padding="10,0"
            Background="{DynamicResource LighCyan}"
            BorderBrush="{DynamicResource Black}"
            BorderThickness="0,0,0,1"
            Foreground="White"
            Duration="60">
            <Lay:LayNoticeBar.Resources>
                <Storyboard
                    x:Key="Storyboard"
                    AutoReverse="True"
                    RepeatBehavior="Forever">
                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Icon" Storyboard.TargetProperty="(UIElement.Opacity)">
                        <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0.5" />
                    </DoubleAnimationUsingKeyFrames>
                </Storyboard>
            </Lay:LayNoticeBar.Resources>
            <Lay:LayNoticeBar.Triggers>
                <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                    <BeginStoryboard Storyboard="{StaticResource Storyboard}" />
                </EventTrigger>
            </Lay:LayNoticeBar.Triggers>
            <Lay:LayButton
                Width="auto"
                Background="Transparent"
                Content="{Binding Message}"
                HoverBackground="Transparent"
                Type="Customize"
                Uri="https://github.com/Layui-WPF-Team/Layui-WPF" />
            <Lay:LayNoticeBar.InnerLeftContent>
                <TextBlock
                    x:Name="Icon"
                    VerticalAlignment="Center"
                    FontFamily="{DynamicResource IconFont}"
                    FontSize="20"
                    Text="&#xeff0;" />
            </Lay:LayNoticeBar.InnerLeftContent>
        </Lay:LayNoticeBar>
        <Lay:LayDrawerHost
            Grid.Row="1"
            DrawerOpen="{Binding ElementName=Title, Path=IsChecked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
            Type="Right">
            <Lay:LayDrawerHost.DrawerContent>
                <Border Width="300" Background="#F7F9F6">
                    <StackPanel>
                        <Image Source="/LayuiApp;component/R-C.jpg" />
                        <TextBlock
                            Margin="10"
                            HorizontalAlignment="Center"
                            FontSize="25"
                            Text="老老实实打代码"
                            TextAlignment="Center"
                            TextWrapping="Wrap" />
                    </StackPanel>
                </Border>
            </Lay:LayDrawerHost.DrawerContent>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Border
                    Height="60"
                    Panel.ZIndex="1"
                    Background="#23262E">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <ToggleButton WindowChrome.IsHitTestVisibleInChrome="true">
                            <ToggleButton.Template>
                                <ControlTemplate TargetType="ToggleButton">
                                    <ContentPresenter />
                                </ControlTemplate>
                            </ToggleButton.Template>
                            <Border Width="200" Background="{DynamicResource LighCyan}">
                                <Border.Effect>
                                    <DropShadowEffect
                                        BlurRadius="3"
                                        Direction="-90"
                                        Opacity="0.6"
                                        ShadowDepth="0" />
                                </Border.Effect>
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    FontSize="20"
                                    Foreground="{DynamicResource Gradient}"
                                    Text="LayUI-WPF" />
                            </Border>
                        </ToggleButton>
                        <Lay:LayButton
                            Grid.Column="1"
                            Margin="10,0"
                            HorizontalAlignment="Left"
                            Background="{DynamicResource Gradient}"
                            BorderThickness="0"
                            Content="关于项目"
                            Cursor="Hand"
                            Foreground="White"
                            HoverBackground="{DynamicResource Gradient}"
                            Type="Customize"
                            Uri="https://github.com/Layui-WPF-Team/Layui-WPF"
                            WindowChrome.IsHitTestVisibleInChrome="True" />
                    </Grid>
                </Border>
                <Border
                    Grid.Row="1"
                    Panel.ZIndex="0"
                    Background="White">
                    <Grid>
                        <Grid Background="White">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Setter Property="Panel.ZIndex" Value="100" />
                                    <Setter Property="Visibility" Value="Collapsed" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding Network}" Value="false">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
